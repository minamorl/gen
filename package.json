{
    "name": "@minamorl/gen",
    "version": "0.1.0",
    "description": "Template generator for TS / Python / Rust scaffolds",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "gen": "./dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsx src/cli.ts",
        "test": "vitest",
        "test:run": "vitest run",
        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "format": "biome format --write ."
    },
    "keywords": [
        "generator",
        "scaffold",
        "typescript",
        "python",
        "rust",
        "cli"
    ],
    "author": "minamorl",
    "license": "MIT",
    "packageManager": "pnpm@10.17.1",
    "engines": {
        "node": ">=20"
    },
    "files": [
        "dist",
        "templates"
    ],
    "dependencies": {
        "@inquirer/prompts": "^8.1.0",
        "commander": "^14.0.2",
        "nunjucks": "^3.2.4"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.11",
        "@types/node": "^25.0.3",
        "@types/nunjucks": "^3.2.6",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "vitest": "^4.0.16"
    }
}
